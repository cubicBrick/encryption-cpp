{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/Fo:",
                "${workspaceFolder}\\objects\\",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}",
                "AdvAPI32.Lib",
                "${workspaceFolder}\\utility\\bigint\\*.cpp",
                //"${workspaceFolder}\\utility\\*.cpp",
                "/I",
                "${workspaceFolder}\\utility\\bigint",
                "/I",
                "${workspaceFolder}\\utility",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}